<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@expense_master, url: yield(:url) ) do |f| %>
      <%= render 'shared/error_messages' , object: f.object %>

      <%= f.label :expense_class_id %>
      <%= f.collection_select :expense_class_id, @expense_classes , :id, :name, {:include_blank => true},{:class => "form-control"} %>

      <%= f.label :name %>
      <%= f.text_field :name ,class:'form-control' %>

      <%= f.label :unit_text %>
      <%= f.text_field :unit_text ,class:'form-control' %>

      <%= f.submit yield(:botton_text) , class: "btn btn-primary" %>
    <% end %>
  </div>
</div>