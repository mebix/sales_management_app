<tr>
  <td class="expense_type"><%= expense.expense_type %></td>
  <td class="billing_at"><%= expense.billing_at.strftime("%Y-%m") %></td>
  <td class="class_name"><%= expense.contract_expense.expense_master.expense_class.name %></td>
  <td class= "name" ><%= link_to expense.contract_expense.expense_master.name, edit_expense_path(expense) %></td>
  <td class="price"><%= expense.price %></td>
  <td class= "destroy" ><%= link_to "delete" , expense, method: :delete, data: { confirm: "You sure?"} %></td>
</tr>
